    <record model="ir.ui.view" id="api_log_tree_view">
        <field name="name">API Logs Tree</field>
        <field name="model">api.logs</field>
        <field name="arch" type="xml">
            <tree>
                <field name="create_date" readonly="1"/>
                <field name="model" readonly="1"/>
                <field name="api_type" readonly="1"/>
                <field name="mocdoc_api_values" readonly="1"/>
                <field name="name" readonly="1"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="api_log_action">
        <field name="name">API Logs</field>
        <field name="res_model">api.logs</field>
        <field name="view_mode">tree</field>
        <!-- <field name="search_view_id" ref="view_printing_lccation_filter"/> -->
    </record>


    <menuitem 
        id="api_logs"
        name="API Logs"
        parent="base.menu_administration"
        action="api_log_action"
        sequence="3"/>

</odoo>
